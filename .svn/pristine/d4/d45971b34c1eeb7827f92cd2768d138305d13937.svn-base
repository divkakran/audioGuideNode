require('../config/app');
function Monument(id , image , desc , cityId , monName){
    this.monumentId     = id;
    if(!image){
        this.monumentsImage = '';
    }else{
        this.monumentsImage =  App.imageTesting+image;
    }
    this.monumentDesc   = desc;
    this.cityId         = cityId;
    this.monumentName   = monName;
}

module.exports = Monument